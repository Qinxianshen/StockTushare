  height: 80rpx;
  margin: 20rpx;
  border-radius: 5px;
  text-align: center;
  border:1px solid #aaa;
}
.journey{
  font-size: 22rpx;
  font-weight: bold;
  line-height: 80rpx;
  color: #000
}
