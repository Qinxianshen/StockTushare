<!--pages/blockchain/blockchain.wxml-->
<text>BTC/USD(比特币/美元)</text>
<view class="navigator" wx:for="{{data}}" wx:for-item="i" wx:for-index="j">
  <view class="item">
      <text class="date">{{data[length-j-1][0]}}</text>
  </view>
  <view class="item">
      <text class="open">开盘：{{data[length-j-1][1]}}</text>
  </view>
  <view class="item">
      <text class="boxoffice">最高：{{data[length-j-1][2]}}</text>
  </view>
  <view class="item">
      <text class="days">最低：{{data[length-j-1][3]}}</text>
  </view>
  <view class="item">
      <text class="days">收盘：{{data[length-j-1][4]}}</text>
  </view>
</view>